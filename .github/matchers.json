{
  "problemMatcher": [
    {
      "owner": "editorconfig",
      "pattern": [
        {
          "regexp": "^([^:]+):(\\d+):(\\d+):\\s+(error|warning|note):\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      ]
    }
  ]
}
